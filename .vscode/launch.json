{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: trello.py",
            "type": "debugpy",
            "request": "launch",
            "program": "cupidone/trello.py",
            "console": "integratedTerminal",
            "args": [
                "${file}"
            ]
        },
        {
            "name": "Python Debugger: build.py",
            "type": "debugpy",
            "request": "launch",
            "program": "cupidone/build.py",
            "console": "integratedTerminal",
            "args": []
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        }
    ]
}